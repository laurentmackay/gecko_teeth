centering 
\backslash
begin{subfigure}[t]{.3
\backslash
columnwidth}
\backslash
caption{ }
\backslash
vspace{0.5em} 
\backslash
centerline{
\end_layout

\end_inset


\begin_inset Graphics
	filename auto/ODE/phase_nonlocal/ftb_eps_kappa=1.pdf
	width 100col%
	special decodearray={.1 .5 .5 .5 .5 .5}

\end_inset


